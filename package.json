    "fs-extra": "^9.0.1",
    "htmlparser2": "^4.1.0",
    "p-queue": "^6.4.0",
    "postcss": "^7.0.32",
    "querystring": "^0.2.0",
    "resolve": "^1.17.0",
    "source-map": "^0.7.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
